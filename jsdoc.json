{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./"],
    "includePattern": "(.js)$",
	"exclude": ["./node_modules", "./deprecated", "./fontconverter", "./tests"],
    "excludePattern": "(node_modules|docs|dist|examples|libs|tests|saucelabs.karma.conf.js|karma.conf.js)"
  },
  "plugins": [
    "node_modules/jsdoc/plugins/markdown"
  ],
  "markdown": {
    "tags": ["param", "deprecated", "description"]
  },
  "opts": {
    "template": "node_modules/docdash/",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
